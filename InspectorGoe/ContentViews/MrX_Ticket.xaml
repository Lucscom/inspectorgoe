<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="InspectorGoe.ContentViews.MrX_Ticket"
             Padding="10">

    <Frame
        BorderColor="Gold"
        CornerRadius="10"
        HeightRequest="150"
        WidthRequest="120"
        BackgroundColor="LightSkyBlue"
        Padding="5"
        HasShadow="True">
        <Image
            Source="{Binding mrXtickets}"/>
    </Frame>

</ContentView>
